{
    "name": "zetrider/nova-inputs-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova",
        "field"
    ],
    "authors": [
        {
            "name": "ZetRider",
            "email": "zetriderman@gmail.com",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "laravel/nova": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ZetRider\\NovaInputsField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ZetRider\\NovaInputsField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}